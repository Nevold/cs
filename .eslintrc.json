{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "plugins": ["html", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "consistent-return": "off",
    "object-curly-newline": "off",
    "linebreak-style": "off",
    "operator-linebreak": "off",
    "no-plusplus": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off"
  }
}
